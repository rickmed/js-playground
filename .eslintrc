{
  "parser": "babel-eslint",
	"env": {
		"node": true
	},
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict ": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "root": root,
	"rules": {
		"indent": [1, 2, {"VariableDeclarator": 2} ],
		"no-unused-vars": 2,
		"curly": [1, "multi-line"],
		"space-infix-ops": 2,
		"comma-spacing": [1, {
				"before": false,
				"after": true
		}],
		"eol-last": 1
	}
}
